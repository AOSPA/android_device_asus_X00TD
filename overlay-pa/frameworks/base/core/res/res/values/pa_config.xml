<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Gestures' key codes. If the gesture is not supported set it to 0. -->
    <integer name="config_doubleTapKeyCode">204</integer>
    <integer name="config_drawInverseVKeyCode">250</integer>
    <integer name="config_drawOKeyCode">249</integer>
    <integer name="config_drawMKeyCode">254</integer>
    <integer name="config_drawVKeyCode">252</integer>
    <integer name="config_drawWKeyCode">253</integer>
    <integer name="config_drawSKeyCode">251</integer>
    <integer name="config_oneFingerSwipeLeftKeyCode">257</integer>
    <integer name="config_oneFingerSwipeDownKeyCode">256</integer>
    <integer name="config_oneFingerSwipeRightKeyCode">258</integer>
    <integer name="config_oneFingerSwipeUpKeyCode">255</integer>

    <!-- Default settings for gestures
         0 - Disabled
         1 - Wake device
         2 - Toggle flashlight
         3 - Toggle airplane mode
         4 - Play/Pause music
         5 - Next music
         6 - Previous music
         7 - Open camera
         8 - Open dialer
    -->
    <integer name="config_doubleTapDefault">1</integer>
    <integer name="config_drawVDefault">2</integer>
    <integer name="config_drawWDefault">8</integer>
    <integer name="config_drawSDefault">3</integer>
    <integer name="config_oneFingerSwipeUpDefault">0</integer>
    <integer name="config_oneFingerSwipeRightDefault">5</integer>
    <integer name="config_oneFingerSwipeLeftDefault">6</integer>
    <integer name="config_oneFingerSwipeDownDefault">0</integer>
    <integer name="config_drawInverseVDefault">0</integer>
    <integer name="config_drawMDefault">0</integer>
    <integer name="config_drawODefault">0</integer>

    <!-- Whether to intercept inputs on screen off -->
    <bool name="config_enableKeyHandler">true</bool>

</resources>
