# Firmware
/firmware                                                                     u:object_r:firmware_file:s0
/bt_firmware                                                                  u:object_r:bt_firmware_file:s0

# Light HAL
/(vendor|system/vendor)/bin/hw/android\.hardware\.light@2\.0-service\.asus_X00TD                u:object_r:hal_light_default_exec:s0

# Sensors
