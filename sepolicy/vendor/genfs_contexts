# Touchpanel
genfscon sysfs /kernel/touchpanel      u:object_r:sysfs_touchpanel:s0

# RTC
genfscon sysfs /devices/platform/soc/800f000.qcom,spmi/spmi-0/spmi0-00/800f000.qcom,spmi:qcom,pm660@0:qcom,pm660_rtc/rtc     u:object_r:sysfs_rtc:s0

# Vibrator
genfscon sysfs /devices/platform/soc/800f000.qcom,spmi/spmi-0/spmi0-01/800f000.qcom,spmi:qcom,pm660@1:qcom,haptics@c000/leds/vibrator/trigger u:object_r:sysfs_leds:s0

# Kgsl
genfscon sysfs /devices/platform/soc/soc:qcom,kgsl-hyp/subsys3/name               u:object_r:sysfs_ssr:s0
genfscon sysfs /devices/platform/soc/soc:qcom,kgsl-hyp/subsys3/restart_level            u:object_r:sysfs_ssr_toggle:s0
